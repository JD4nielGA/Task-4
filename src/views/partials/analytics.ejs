<script async src="https://www.googletagmanager.com/gtag/js?id=<%= gaKey %>"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', '<%= gaKey %>', {
        cookie_domain: '<%= cD %>',
        cookie_flags: 'SameSite=None; Secure',
        debug_mode: true 
      });
    </script>
